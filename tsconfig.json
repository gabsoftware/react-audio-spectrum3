{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["ES2020", "DOM"],
    "allowJs": true,
    "jsx": "react",
    "declaration": true,
    "outDir": "./lib",
    "rootDir": "./src",
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    // "types": [],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
